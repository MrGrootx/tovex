{
  "name": "tovex",
  "description": "Tovex is Utility for your projects",
  "version": "1.0.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "import": "./dist/types.mjs",
      "require": "./dist/types.cjs"
    }
  },
  "repository": "https://github.com/MrGrootx/tovex",
  "homepage": "https://github.com/MrGrootx/tovex",
  "bugs": "https://github.com/MrGrootx/tovex/issues",
  "keywords": [
    "tovex",
    "utility",
    "number",
    "format"
  ],
  "files": [
    "dist"
  ],

  "scripts": {
    "build": "tsup",
    "prepublishOnly": "bun run build"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "tsup": "^8.5.1"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "author": "justgroot",
  "license": "ISC"
}
